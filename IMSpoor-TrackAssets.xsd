							<xs:documentation>Het type Lichtbak. De lijst van opties is voorgedefiniÃ«erd.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="text" type="tString50" use="required">
						<xs:annotation>
							<xs:documentation>tekst van de lichtbak</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="isIntegrated" type="tTrueFalseEnum" use="required">
						<xs:annotation>
							<xs:documentation>Indicatie of de lichtbak op de seinpaal is gemonteerd.</xs:documentation>
						</xs:annotation>
