<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.prorail.nl/IMSpoor" 
    xmlns:xs="http://www.w3.org/2001/XMLSchema" 
    xmlns:xml="http://www.w3.org/XML/1998/namespace" targetNamespace="http://www.prorail.nl/IMSpoor" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="https://raw.githubusercontent.com/XanderdenDuijn/IMX/master/IMSpoor-BasicTypes.xsd"/>
	<xs:include schemaLocation="https://raw.githubusercontent.com/XanderdenDuijn/IMX/master/IMSpoor-Infra.xsd"/>
	<xs:annotation>
		<xs:documentation> De elementen in deze XSD bevatten de spoorbeleggingen, objecten direct gerelateerd aan het spoornetwerk, zoals seinen en borden, die op de netwerktopologie worden geprojecteerd. Let op: alle geografische gegevens worden gedefinieerd in GML.
		</xs:documentation>
	</xs:annotation>
	<xs:element name="TrackAssets">
		<xs:annotation>
			<xs:documentation>Een verzameling objecten met een directe relatie tot een spoortak</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Signals" minOccurs="0"/>
				<xs:element ref="DepartureSignals" minOccurs="0"/>
				<xs:element ref="Signs" minOccurs="0"/>
				<xs:element ref="StopMarkerBoards" minOccurs="0"/>
				<xs:element ref="BaliseGroups" minOccurs="0"/>
				<xs:element ref="LevelCrossings" minOccurs="0"/>
				<xs:element ref="Platforms" minOccurs="0"/>
				<xs:element ref="PlatformWalls" minOccurs="0"/>
				<xs:element ref="InsulatedJoints" minOccurs="0"/>
				<xs:element ref="AudioFrequencyJoints" minOccurs="0"/>
				<xs:element ref="AxleCounterDetectionPoints" minOccurs="0"/>
				<xs:element ref="ATBVVInstallations" minOccurs="0"/>
				<xs:element ref="ATBNGBeacons" minOccurs="0"/>
				<xs:element ref="ATBNGLoops" minOccurs="0"/>
				<xs:element ref="Detectors" minOccurs="0"/>
				<xs:element ref="Pedals" minOccurs="0"/>
				<xs:element ref="OverlayTrackCircuits" minOccurs="0"/>
				<xs:element ref="MassDetectionLoops" minOccurs="0"/>
				<xs:element ref="PermanentWarningSystems" minOccurs="0"/>
				<xs:element ref="FlankProtectionSystems" minOccurs="0"/>
				<xs:element ref="SofisBalises" minOccurs="0"/>
				<xs:element ref="VoltageTransitions" minOccurs="0"/>
				<xs:element ref="GuardRails" minOccurs="0"/>
				<xs:element ref="ExpansionInstallations" minOccurs="0"/>
				<xs:element ref="PhaseBreaks" minOccurs="0"/>
				<xs:element ref="FloodBarriers" minOccurs="0"/>
				<xs:element ref="LubricationNozzles" minOccurs="0"/>
				<xs:element ref="BridgeTransitions" minOccurs="0"/>
				<xs:element ref="MonitoringLocations" minOccurs="0"/>
				<xs:element ref="Crocodiles" minOccurs="0"/>
				<xs:element ref="Indusis" minOccurs="0"/>
				<xs:element ref="CompressedAirConnections" minOccurs="0"/>
				<xs:element ref="OverheadLineEnds" minOccurs="0"/>
				<xs:element ref="RailCoils" minOccurs="0"/>
				<xs:element ref="BridgeWarningSystems" minOccurs="0"/>
				<xs:element ref="LevelCrossingWarningSystems" minOccurs="0"/>
				<xs:element ref="SightRestrictingObjectWarningSystems" minOccurs="0"/>
				<xs:element ref="TunnelWarningSystems" minOccurs="0"/>
				<xs:element ref="ATBEGLoops" minOccurs="0"/>
				<xs:element ref="LoadingGauges" minOccurs="0"/>
				<xs:element ref="Blockings" minOccurs="0"/>
				<xs:element ref="FictitiousSignals" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Blockings">
		<xs:annotation>
			<xs:documentation>Een verzameling stoppingen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Blocking" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Blocking">
		<xs:annotation>
			<xs:documentation>Stopping. Een versperring in het het spoor om een deel van het spoor onbereikbaar te maken.</xs:documentation>
			<xs:documentation xml:lang="NL">Stopping</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="LoadingGauges">
		<xs:annotation>
			<xs:documentation>Een verzameling Ladingmallen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="LoadingGauge" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="LoadingGauge">
		<xs:annotation>
			<xs:documentation>Ladingmal. Een voorziening om te bepalen of een geladen goederenwagen binnen het profiel van de vrije ruimte valt.</xs:documentation>
			<xs:documentation xml:lang="NL">Ladingmal</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="ATBEGLoops">
		<xs:annotation>
			<xs:documentation>Een verzameling ATB-EG Lussen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ATBEGLoop" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ATBEGLoop">
		<xs:annotation>
			<xs:documentation>ATB-EG Lus is een voorziening om opeenvolgende code hikken op te vangen. ATB-Lussen worden ook gebruikt om ATB-codes te kunnen zenden waar dat niet via de rails gerealiseerd kan worden.</xs:documentation>
			<xs:documentation xml:lang="NL">ATB-EG Lus</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tATBLoop"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="BridgeWarningSystems">
		<xs:annotation>
			<xs:documentation>Een verzameling waarschuwingsinstallaties voor bruggen.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BridgeWarningSystem" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BridgeWarningSystem">
		<xs:annotation>
			<xs:documentation>WaarschuwingsInstallatie Brug</xs:documentation>
			<xs:documentation xml:lang="NL">WIBR</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset">
					<xs:attribute name="civilConstructionRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>PUIC verwijzing naar de bijbehorende brug.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="hasBell" type="tTrueFalseEnum" use="required">
						<xs:annotation>
							<xs:documentation>Indicatie van de aanwezigheid van een schel bij de WIBR.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="LevelCrossingWarningSystems">
		<xs:annotation>
			<xs:documentation>Een verzameling waarschuwingsinstallaties voor dienstoverpaden.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="LevelCrossingWarningSystem" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="LevelCrossingWarningSystem">
		<xs:annotation>
			<xs:documentation>WaarschuwingsInstallatie Dienstoverpad</xs:documentation>
			<xs:documentation xml:lang="NL">WIDO</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset">
					<xs:attribute name="levelCrossingRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>PUIC verwijzing naar de bijbehorende overpad/overweg.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="levelCrossingWarningInstallationType" type="tLevelCrossingWarningInstallationTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>Type WaarschuwingsInstallatie Dienstoverpad. De lijst van opties is voorgedefiniÃ«erd.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SightRestrictingObjectWarningSystems">
		<xs:annotation>
			<xs:documentation>Een verzameling waarschuwingsinstallaties voor uitzichtbelemmerende objecten</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SightRestrictingObjectWarningSystem" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SightRestrictingObjectWarningSystem">
		<xs:annotation>
			<xs:documentation>Waarschuwingsinstallatie Uitzichtbelemmerende objecten</xs:documentation>
			<xs:documentation xml:lang="NL">WUBO</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="TunnelWarningSystems">
		<xs:annotation>
			<xs:documentation>Een verzameling waarschuwingsinstallaties voor tunnels.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="TunnelWarningSystem" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TunnelWarningSystem">
		<xs:annotation>
			<xs:documentation>WaarschuwingsInstallatie Tunnel</xs:documentation>
			<xs:documentation xml:lang="NL">WIT</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset">
					<xs:attribute name="tunnelTubeRef" type="tUUIDRef" use="required">
						<xs:annotation>
							<xs:documentation>PUIC verwijzing naar de bijbehorende tunnelbuis.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="OverheadLineEnds">
		<xs:annotation>
			<xs:documentation>Een verzameling eindpunten van de bovenleiding</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="OverheadLineEnd" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="OverheadLineEnd">
		<xs:annotation>
			<xs:documentation>Eind (of begin) -punt van de bovenleiding</xs:documentation>
			<xs:documentation xml:lang="NL">Einde Bovenleiding</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="RailCoils">
		<xs:annotation>
			<xs:documentation>Een verzameling Railspoelen.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RailCoil" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RailCoil">
		<xs:annotation>
			<xs:documentation>RailSpoel</xs:documentation>
			<xs:documentation xml:lang="NL">RailSpoel</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CompressedAirConnections">
		<xs:annotation>
			<xs:documentation>Een verzameling persluchtaansluitingen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CompressedAirConnection" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CompressedAirConnection">
		<xs:annotation>
			<xs:documentation>Een persluchtaansluiting tbv een rembeproevingsinstallatie.</xs:documentation>
			<xs:documentation xml:lang="NL">Persluchtaansluiting</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset">
					<xs:attribute name="brakeTestingInstallationRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>PUIC verwijzing naar de bijbehorende Rembeproevingsinstallatie</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="MonitoringLocations">
		<xs:annotation>
			<xs:documentation>Een verzameling monitoringlocaties.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MonitoringLocation" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MonitoringLocation">
		<xs:annotation>
			<xs:documentation>Een monitoring locatie, hier kunnen een combinatie van Quo-Vadis en Hotbox meet apparatuur staan.</xs:documentation>
			<xs:documentation xml:lang="NL">Monitoringlocatie</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset">
					<xs:sequence>
						<xs:element ref="TagReader" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="QuoVadis" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="HotboxDetector" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="contractor" type="tString50">
						<xs:annotation>
							<xs:documentation>De aannemer die verantwoordelijk is voor de monitoringlocatie.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="TagReader">
		<xs:annotation>
			<xs:documentation>Instrument voor het uitlezen van de RFID-tag op een passerende trein.</xs:documentation>
			<xs:documentation xml:lang="NL">Tagreader</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="QuoVadis">
		<xs:annotation>
			<xs:documentation>Een meetsysteem met een glasvezelsensor dat onder meer het gewicht van een passerende trein kan bepalen.</xs:documentation>
			<xs:documentation xml:lang="NL">Quo Vadis</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset">
					<xs:sequence>
						<xs:element ref="QuoVadisSensor" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="hasActiveSignaling" type="tTrueFalseEnum" use="required"/>
					<xs:attribute name="monitoringSystemCabinetRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>PUIC verwijzing naar de gerelateerde Systeemkast</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="QuoVadisSensor">
		<xs:annotation>
			<xs:documentation>De sensor van een Quo Vadis-installatie meet het gewicht van een passerende trein op basis van de doorbuiging van het spoor.</xs:documentation>
			<xs:documentation xml:lang="NL">Sensor</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="HotboxDetector">
		<xs:annotation>
			<xs:documentation>Een Hotbox detector controleert of een passerende trein een te hete as of rem heeft.</xs:documentation>
			<xs:documentation xml:lang="NL">Hotbox Detector</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset">
					<xs:sequence>
						<xs:element ref="Scanner" minOccurs="0"/>
						<xs:element ref="RailContact" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="hasActiveSignaling" type="tTrueFalseEnum" use="required"/>
					<xs:attribute name="monitoringSystemCabinetRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>PUIC verwijzing naar de gerelateerde Systeemkast</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Scanner">
		<xs:annotation>
			<xs:documentation>De scanner die de temperatuur van de rem en de as van een voorbij rijdende trein meet.</xs:documentation>
			<xs:documentation xml:lang="NL">Scanner</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="RailContact">
		<xs:annotation>
			<xs:documentation>Het railcontact staat op afstand van de Hotbox-installatie en zorgt voor een sein naar de scanner, zodat deze open gaat of dicht kan.</xs:documentation>
			<xs:documentation xml:lang="NL">Railcontact</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="BridgeTransitions">
		<xs:annotation>
			<xs:documentation>Een verzameling Brugovergangen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BridgeTransition" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BridgeTransition">
		<xs:annotation>
			<xs:documentation>Een Brugovergang</xs:documentation>
			<xs:documentation xml:lang="NL">Brugovergang</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset">
					<xs:attribute name="rail" type="tRailEnum" use="required">
						<xs:annotation>
							<xs:documentation>Indicatie in welke spoorstaaf de brugovergang is gemonteerd</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="civilConstructionRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>PUIC verwijzing naar de bijbehorende brug.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="LubricationNozzles">
		<xs:annotation>
			<xs:documentation>Een verzameling SpoorStaaf ConditioneringsSystem of SSCS spuitkoppen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="LubricationNozzle" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="LubricationNozzle">
		<xs:annotation>
			<xs:documentation>De spuitkop van een Spoortstaaf ConditioneringsSysteem</xs:documentation>
			<xs:documentation xml:lang="NL">Spuitkop</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset">
					<xs:attribute name="lubricationSystemCabinetRef" type="tUUIDRef">
						<xs:annotation>
							<xs:documentation>PUIC verwijzing naar de gerelateerde SSCS-kast</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="FloodBarriers">
		<xs:annotation>
			<xs:documentation>Een lijst van waterkeringen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FloodBarrier" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FloodBarrier">
		<xs:annotation>
			<xs:documentation>Een waterkering is een beweegbare afsluiting in tunnels om bescherming te bieden bij overstomingen.</xs:documentation>
			<xs:documentation xml:lang="NL">Waterkering</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="PhaseBreaks">
		<xs:annotation>
			<xs:documentation>Een lijst van fasescheidingen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="PhaseBreak" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PhaseBreak">
		<xs:annotation>
			<xs:documentation>Een fasescheiding wordt gebruikt bij de overgang naar een ander onderstation.</xs:documentation>
			<xs:documentation xml:lang="NL">Fasescheiding</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointTrackAsset"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExpansionInstallations">
		<xs:annotation>
			<xs:documentation>Een lijst van compensatie/dilaterende inrichtingen</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ExpansionInstallation" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExpansionInstallation">
		<xs:annotation>
			<xs:documentation>Een compensatieinrichting is een technische constructie ter compensatie van uitzetting en inkrimping van de
spoorstaven als gevolg van temperatuur wisselingen over grotere lengte met oplegging op minstens 7 dwarsliggers
en bestaande uit twee of meer compensatielassen.</xs:documentation>
			<xs:documentation xml:lang="NL">Dilaterende Inrichting</xs:documentation>
